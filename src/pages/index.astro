import FullDevCycle from "../components/blocks/FullDevCycle.astro";
import WhatWeDo from "../components/blocks/WhatWeDo.astro";
import Clients from "../components/blocks/Clients.astro";
import Career from "../components/blocks/Career.astro";
import ContactForm from "../components/blocks/ContactForm.astro";
import Footer from "../components/blocks/Footer.astro";
---

<Layout>
	<Hero/>
